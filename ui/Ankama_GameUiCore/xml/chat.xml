        <!-- Button btn_menu-->

        <Button name="btn_menu">
            <Common>
                <checkBox>true</checkBox>
                <Anchors>
                    <Anchor relativePoint="BOTTOMLEFT">
                        <AbsDimension x="0" y="-24" />
                    </Anchor>
                </Anchors>
                <Texture name="tx_btn_menu">
                    <Size>
                        <AbsDimension x="23" y="22" />
                    </Size>
                    <uri>[local.assets]tchat_btn_menu</uri>
                </Texture>
                <Texture name="tx_arrow_btn_menu">
                    <Anchors>
                        <Anchor>
                            <AbsDimension x="0" y="0" />
                        </Anchor>
                    </Anchors>
                    <Size>
                        <AbsDimension x="23" y="22" />
                    </Size>
                    <uri>[local.assets]tchat_tx_petitefleche</uri>
                    <gotoAndStop>normal</gotoAndStop>
                </Texture>
                <Hooks>
                    <onRelease />
                    <onRollOver />
                    <onRollOut />
                </Hooks>
            </Common>
            <State type="OVER">
                <SetProperties target="tx_btn_menu">
                    <gotoAndStop>over</gotoAndStop>
                </SetProperties>
            </State>
            <State type="PRESSED">
                <SetProperties target="tx_btn_menu">
                    <gotoAndStop>pressed</gotoAndStop>
                </SetProperties>
            </State>
            <State type="SELECTED">
                <SetProperties target="tx_arrow_btn_menu">
                    <gotoAndStop>selected</gotoAndStop>
                </SetProperties>
            </State>
            <State type="SELECTED_OVER">
                <SetProperties target="tx_btn_menu">
                    <gotoAndStop>over</gotoAndStop>
                </SetProperties>
            </State>
            <State type="SELECTED_PRESSED">
                <SetProperties target="tx_btn_menu">
                    <gotoAndStop>pressed</gotoAndStop>
                </SetProperties>
            </State>
        </Button>


