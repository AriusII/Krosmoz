<?xml version="1.0" encoding="UTF-8"?>
<Definition>

	<Import url="[config.mod.template]button/simpleButton.xml" />
	<Import url="[config.mod.template]button/iconButton.xml" />
	<Import url="[config.mod.template]button/sortButton.xml" />
	
   	<Constants>
		<Constant name="assets" value="[config.ui.skin]assets.swf|" />
       <Constant name="css" value="[config.ui.skin]css/" />
   	</Constants>

	<Container>
		<Anchors>
			<Anchor point="CENTER" relativePoint="CENTER">
				<AbsDimension x="0" y="-40" />
			</Anchor>
		</Anchors>		
		<mouseEnabled>true</mouseEnabled>

		<!-- Texture tx_bg-->

		<Texture>
			<Size>
				<AbsDimension x="853" y="755" />
			</Size>
			<autoGrid>true</autoGrid>
			<uri>[local.assets]tx_generalBackgroundWithTitle</uri>
		</Texture>
		
		
		<!-- Label lbl_title-->

		<Label>
			<Anchors>
				<Anchor>
					<AbsDimension x="37" y="15" />
				</Anchor>
			</Anchors>
			<Size>
				<AbsDimension x="563" y="35" />
			</Size>
			<text>[ui.estate.agency]</text>
			<css>[local.css]title.css</css>
		</Label>



		<!-- Label lbl_estateType-->

		<Label>
			<Anchors>
				<Anchor>
					<AbsDimension x="30" y="80" />
				</Anchor>
			</Anchors>
			<Size>
				<AbsDimension x="410" y="20" />
			</Size>
			<text>[ui.estate.typeChoice]</text>
			<css>[local.css]normal.css</css>
		</Label>
			
		<!-- ComboBox cb_estateType-->

		<ComboBox name="cb_estateType" strata="TOP">
			<Anchors>
				<Anchor>
					<AbsDimension x="450" y="80" />
				</Anchor>
			</Anchors>
			<Size>
				<AbsDimension x="380" y="25" />
			</Size>
			<autoSelectMode>0</autoSelectMode>
			<buttonTexture>[local.assets]btn_combobox</buttonTexture>
			<listTexture>[local.assets]tx_combobox</listTexture>
			<rendererArgs>[local.css]normal.css,,,[colors.grid.over],[colors.grid.selected]</rendererArgs>;
			<scrollBarCss>[local.css]scrollBar.css</scrollBarCss>
			<Hooks>
				<onSelectItem />
			</Hooks>
		</ComboBox>





		<Container name="listCtr">
			<Anchors>
				<Anchor>
					<AbsDimension x="24" y="261" />
				</Anchor>
			</Anchors>

			<!-- Texture tx_bg1-->

			<Texture>
				<Size>
					<AbsDimension x="807" y="474" />
				</Size>
				<autoGrid>true</autoGrid>
				<uri>[local.assets]tx_blockBackground</uri>
			</Texture>


			<!-- Label lbl_name-->

			<Label>
				<Anchors>
					<Anchor>
						<AbsDimension x="24" y="16" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="200" y="20" />
				</Size>
				<text>[ui.common.name]</text>
				<css>[local.css]normal.css</css>
			</Label>

			<!-- Label lbl_loc-->

			<Label>
				<Anchors>
					<Anchor>
						<AbsDimension x="384" y="16" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="200" y="20" />
				</Size>
				<text>[ui.map.subarea]</text>
				<css>[local.css]normal.css</css>
			</Label>

			<!-- Label lbl_cost-->

			<Label>
				<Anchors>
					<Anchor>
						<AbsDimension x="624" y="16" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="120" y="20" />
				</Size>
				<text>[ui.common.price]</text>
				<css>[local.css]normal.css</css>
			</Label>



			<!-- Grid gd_estates-->

			<Grid name="gd_estates">
				<Anchors>
					<Anchor>
						<AbsDimension x="14" y="42" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="780" y="360" />
				</Size>
				<rendererName>InlineXmlGridRender</rendererName>
				<rendererArgs>updateEstateLine,[colors.grid.line]</rendererArgs>
				<slotWidth>780</slotWidth>
				<slotHeight>36</slotHeight>

				<Container name="estateCtr">
					<Size>
						<AbsDimension x="780" y="36" />
					</Size>


					<!-- Label lbl_name-->

					<Label name="lbl_name">
						<Anchors>
							<Anchor>
								<AbsDimension x="10" y="6" />
							</Anchor>
						</Anchors>
						<Size>
							<AbsDimension x="350" y="20" />
						</Size>
						<css>[local.css]normal.css</css>
					</Label>

					<!-- Label lbl_loc-->

					<Label name="lbl_loc">
						<Anchors>
							<Anchor>
								<AbsDimension x="370" y="6" />
							</Anchor>
						</Anchors>
						<Size>
							<AbsDimension x="220" y="20" />
						</Size>
						<css>[local.css]normal.css</css>
					</Label>

					<!-- Label lbl_cost-->

					<Label name="lbl_cost">
						<Anchors>
							<Anchor>
								<AbsDimension x="610" y="6" />
							</Anchor>
						</Anchors>
						<Size>
							<AbsDimension x="130" y="20" />
						</Size>
						<css>[local.css]normal.css</css>
					</Label>

					<!-- simpleButton btn_more-->

					<iconButton name="btn_more">
						<Anchors>
							<Anchor>
								<AbsDimension x="750" y="8" />
							</Anchor>
						</Anchors>
						<Size>
							<AbsDimension x="20" y="20" />
						</Size>
						<uri>[local.assets]btn_etc_square</uri>
						<Hooks>
							<onRelease />
						</Hooks>
					</iconButton>
				</Container>
			</Grid>

			<!-- Texture tx_decoGrid-->

			<Texture name="tx_decoGrid">
				<Anchors>
					<Anchor>
						<AbsDimension x="12" y="12" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="783" y="418" />
				</Size>
				<uri>[local.assets]tx_commonGrid</uri>
				<autoGrid>true</autoGrid>
			</Texture>




			<sortButton name="btn_tabName">
				<Anchors>
					<Anchor>
						<AbsDimension x="24" y="14" />
					</Anchor>
				</Anchors>				
				<Size>
					<AbsDimension x="340" y="24" />
				</Size>
			</sortButton>

			<sortButton name="btn_tabLoc">
				<Anchors>
					<Anchor>
						<AbsDimension x="354" y="14" />
					</Anchor>
				</Anchors>				
				<Size>
					<AbsDimension x="230" y="24" />
				</Size>
			</sortButton>

			<sortButton name="btn_tabCost">
				<Anchors>
					<Anchor>
						<AbsDimension x="624" y="14" />
					</Anchor>
				</Anchors>				
				<Size>
					<AbsDimension x="160" y="24" />
				</Size>
			</sortButton>



			<!-- iconButton btn_prevPage-->

			<iconButton name="btn_prevPage">
				<Anchors>
					<Anchor point="TOP" relativePoint="BOTTOM" relativeTo="tx_decoGrid">
						<AbsDimension x="-50" y="10" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="20" y="27" />
				</Size>
				<uri>[local.assets]btn_bigArrow_left</uri>
			</iconButton>
			
			<!-- iconButton btn_nextPage-->

			<iconButton name="btn_nextPage">
				<Anchors>
					<Anchor point="TOP" relativePoint="BOTTOM" relativeTo="tx_decoGrid">
						<AbsDimension x="50" y="10" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="20" y="27" />
				</Size>
				<uri>[local.assets]btn_bigArrow_right</uri>
			</iconButton>

			<!-- Label lbl_page-->

			<Label name="lbl_page">
				<Anchors>
					<Anchor point="TOP" relativePoint="BOTTOM" relativeTo="tx_decoGrid">
						<AbsDimension x="0" y="10" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="80" y="20" />
				</Size>
				<css>[local.css]normal.css</css>
				<cssClass>center</cssClass>
			</Label>


		</Container>









		<Container name="filterCtr">
			<Anchors>
				<Anchor>
					<AbsDimension x="24" y="116" />
				</Anchor>
			</Anchors>

			<!-- Texture tx_bg2-->

			<Texture>
				<Size>
					<AbsDimension x="807" y="135" />
				</Size>
				<autoGrid>true</autoGrid>
				<uri>[local.assets]tx_blockBackground</uri>
			</Texture>

			<!-- Label lbl_filterTitle-->

			<Label>
				<Anchors>
					<Anchor>
						<AbsDimension x="20" y="10" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="550" y="25" />
				</Size>
				<text>[ui.search.criteria]</text>
				<css>[local.css]titleSmall.css</css>
			</Label>
			
			
			
			<!-- Label lbl_filterMaxPrice-->

			<Label>
				<Anchors>
					<Anchor>
						<AbsDimension x="20" y="40" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="210" y="20" />
				</Size>
				<text>[ui.estate.filter.maxPrice]</text>
				<css>[local.css]normal.css</css>
			</Label>
			
			<!-- Texture tx_input-->

			<Texture>
				<Anchors>
					<Anchor>
						<AbsDimension x="250" y="40" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="340" y="25" />
				</Size>
				<uri>[local.assets]tx_inputBackgroundDark</uri>
				<autoGrid>true</autoGrid>
			</Texture>

			<!-- Label inp_filterMaxPrice-->

			<Input name="inp_filterMaxPrice">
				<Anchors>
					<Anchor>
						<AbsDimension x="260" y="39" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="320" y="23" />
				</Size>
				<maxChars>19</maxChars>
				<css>[local.css]normal.css</css>
				<cssClass>right</cssClass>
			</Input>


			<!-- ComboBox cb_filterSubarea-->

			<ComboBox name="cb_filterSubarea" strata="TOP">
				<Anchors>
					<Anchor>
						<AbsDimension x="20" y="95" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="280" y="25" />
				</Size>
				<autoSelectMode>0</autoSelectMode>
				<buttonTexture>[local.assets]btn_combobox</buttonTexture>
				<listTexture>[local.assets]tx_combobox</listTexture>
				<rendererArgs>[local.css]normal.css,,,[colors.grid.over],[colors.grid.selected]</rendererArgs>;
				<scrollBarCss>[local.css]scrollBar.css</scrollBarCss>
				<Hooks>
					<onSelectItem />
				</Hooks>
			</ComboBox>

			<!-- ComboBox cb_filterSkill-->

			<ComboBox name="cb_filterSkill" strata="TOP">
				<Anchors>
					<Anchor>
						<AbsDimension x="310" y="95" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="280" y="25" />
				</Size>
				<autoSelectMode>0</autoSelectMode>
				<buttonTexture>[local.assets]btn_combobox</buttonTexture>
				<listTexture>[local.assets]tx_combobox</listTexture>
				<rendererArgs>[local.css]normal.css,,,[colors.grid.over],[colors.grid.selected]</rendererArgs>;
				<scrollBarCss>[local.css]scrollBar.css</scrollBarCss>
				<Hooks>
					<onSelectItem />
				</Hooks>
			</ComboBox>


			<!-- ComboBox cb_filterRoomMount-->

			<ComboBox name="cb_filterRoomMount" strata="TOP">
				<Anchors>
					<Anchor>
						<AbsDimension x="20" y="70" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="280" y="25" />
				</Size>
				<autoSelectMode>0</autoSelectMode>
				<buttonTexture>[local.assets]btn_combobox</buttonTexture>
				<listTexture>[local.assets]tx_combobox</listTexture>
				<rendererArgs>[local.css]normal.css,,,[colors.grid.over],[colors.grid.selected]</rendererArgs>;
				<scrollBarCss>[local.css]scrollBar.css</scrollBarCss>
				<Hooks>
					<onSelectItem />
				</Hooks>
			</ComboBox>
			
			<!-- ComboBox cb_filterChestMachine-->

			<ComboBox name="cb_filterChestMachine" strata="TOP">
				<Anchors>
					<Anchor>
						<AbsDimension x="310" y="70" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="280" y="25" />
				</Size>
				<autoSelectMode>0</autoSelectMode>
				<buttonTexture>[local.assets]btn_combobox</buttonTexture>
				<listTexture>[local.assets]tx_combobox</listTexture>
				<rendererArgs>[local.css]normal.css,,,[colors.grid.over],[colors.grid.selected]</rendererArgs>;
				<scrollBarCss>[local.css]scrollBar.css</scrollBarCss>
				<Hooks>
					<onSelectItem />
				</Hooks>
			</ComboBox>



			<!-- simpleButton btn_filter-->

			<simpleButton name="btn_filter" label="[ui.common.search]"   >
				<Anchors>
					<Anchor>
						<AbsDimension x="600" y="90" />
					</Anchor>
				</Anchors>
				<Size>
					<AbsDimension x="190" y="30" />
				</Size>
				<css>[local.css]button.css</css>
			</simpleButton>


		</Container>
		
		
		

		<!-- iconButton btn_close-->

		<iconButton name="btn_close">
			<Anchors>
				<Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" />
			</Anchors>
			<Size>
				<AbsDimension x="57" y="36" />
			</Size>
			<uri>[local.assets]btn_cornerCross</uri>
        </iconButton>
	</Container>
</Definition>